<head>
  <meta charset="utf-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge" />
  <title><%= config.title %></title>
  <meta name="description" content="<%= config.description %>" />
  <meta name="keywords" content="<%= (config.keywords || []).join(',') %>" />
  <meta name="HandheldFriendly" content="True" />
  <meta name="apple-mobile-web-app-capable" content="yes">
  <%- favicon_tag(config.favicon) %>
  <%- feed_tag(config.feed.path, {title: config.title}) %>
  <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
  <%- open_graph({
    twitter_id: theme.twitter,
    google_plus: theme.google_plus,
    fb_admins: theme.fb_admins,
    fb_app_id: theme.fb_app_id
  }) %>
  <!-- close console output -->
  <%- js('js/console') %>
  <!-- font -->
  <!-- chinese font -->
  <link href="https://fonts.loli.net/css?family=Noto+Serif+SC" rel="stylesheet">
  <!-- english font -->
  <link href="https://fonts.loli.net/css?family=Titillium+Web" rel="stylesheet">
  <!-- code font -->
  <link href="https://fonts.loli.net/css?family=Roboto+Mono" rel="stylesheet">
  <!-- icon font -->
  <link href="https://fonts.loli.net/css?family=Material+Icons" rel="stylesheet">
  <!-- progress bar -->
  <link href="https://cdn.bootcss.com/nprogress/0.2.0/nprogress.min.css" rel="stylesheet">
  <script src="https://cdn.bootcss.com/nprogress/0.2.0/nprogress.min.js"></script>
  <!-- wave effect -->
  <link href="https://cdn.bootcss.com/node-waves/0.7.6/waves.min.css" rel="stylesheet">
  <script src="https://cdn.bootcss.com/node-waves/0.7.6/waves.min.js"></script>
  <!-- reveal effect -->
  <script src="https://cdn.bootcss.com/scrollReveal.js/4.0.5/scrollreveal.min.js"></script>
  <!-- jquery -->
  <script src="https://cdn.bootcss.com/jquery/3.4.0/jquery.min.js"></script>
  <!-- image lazyload -->
  <script src="https://cdn.bootcss.com/jquery_lazyload/1.9.7/jquery.lazyload.min.js"></script>
  <!-- image fancybox -->
  <link href="https://cdn.bootcss.com/fancybox/3.5.7/jquery.fancybox.min.css" rel="stylesheet">
  <script src="https://cdn.bootcss.com/fancybox/3.5.7/jquery.fancybox.min.js"></script>
  <!-- aplayer -->
  <link href="https://cdn.bootcss.com/aplayer/1.10.1/APlayer.min.css" rel="stylesheet">
  <script src="https://cdn.bootcss.com/aplayer/1.10.1/APlayer.min.js"></script>
  <!-- gitalk -->
  <link href="https://cdn.bootcss.com/gitalk/1.5.0/gitalk.min.css" rel="stylesheet">
  <script src="https://cdn.bootcss.com/gitalk/1.5.0/gitalk.min.js"></script>
  <!-- busuanzi -->
  <!-- <script async src="https://busuanzi.ibruce.info/busuanzi/2.3/busuanzi.pure.mini.js"></script> -->
  <!-- preload -->
  <!-- <script src="https://cdn.bootcss.com/PreloadJS/1.0.1/preloadjs.min.js"></script> -->
  <!-- pjax -->
  <script src="https://cdn.bootcss.com/jquery.pjax/2.0.1/jquery.pjax.min.js"></script>
  <!-- basic config -->
  <script>
    let ROOT = "<%= config.root %>" || "/";
    if (!ROOT.endsWith('/')) ROOT += '/';
    let CONFIG = {
      search_service: "<%= theme.search.service %>",
      since: '<%= theme.footer.since %>',
      mathjax_cdn: '<%= theme.mathjax.cdn %>',
      poem_api: '<%= theme.poem.api %>'
    }
  </script>
  <!-- js -->
  <%- js('js/search') %>
  <%- js('js/app') %>
  <!-- css -->
  <%- css('style.css') %>
</head>